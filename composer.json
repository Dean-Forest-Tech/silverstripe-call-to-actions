{
  "name": "dft/silverstripe-call-to-actions",
  "description": "A Silverstripe module allowing you to create call to actions and render them using custom templates",
  "type": "silverstripe-vendormodule",
  "homepage": "http://github.com/dft/silverstripe-call-to-actions",
  "keywords": [
    "silverstripe",
    "CTA",
    "Call To Action"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Dean Forest Tech",
      "homepage": "https://dean-forest-tech.uk"
    }
  ],
  "support": {
    "issues": "http://github.com/dft/silverstripe-call-to-actions/issues"
  },
  "require": {
    "silverstripe/framework": "^4|^5",
    "silverstripe/siteconfig": "^4|^5",
    "gorriecoe/silverstripe-linkfield": "^1",
		"symbiote/silverstripe-gridfieldextensions": "^3|^4"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3"
  },
  "autoload": {
    "psr-4": {
      "DFT\\SilverStripe\\CallToActions\\": "src/",
      "DFT\\SilverStripe\\CallToActions\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "lint": "vendor/bin/phpcs src/ tests/",
    "lint-clean": "vendor/bin/phpcbf src/ tests/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
